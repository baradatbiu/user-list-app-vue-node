{
  "name": "backend",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "serve": "nodemon app.js",
    "start:docker": "docker-compose up",
    "stop:docker": "docker-compose down",
    "db:create": "docker exec -it local_db psql -U postgres -c 'CREATE DATABASE ulapi;'",
    "db:delete": "docker exec -it local_db psql -U postgres -c 'DROP DATABASE ulapi (FORCE);'"
  },
  "dependencies": {
    "cache-manager": "^3.4.4",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "pg-promise": "^10.11.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.14"
  }
}
